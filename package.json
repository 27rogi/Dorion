{
  "name": "dorion",
  "private": true,
  "version": "0.3.1",
  "type": "module",
  "scripts": {
    "prebuild": "uglifyjs --comments all --mangle -o src-tauri/injection/injection_min.js -- src-tauri/injection/injection.js",
    "dev": "yarn build && tauri dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "cargo fmt --manifest-path ./src-tauri/Cargo.toml --all",
    "lint": "eslint ./src ./src-tauri/injection/injection.js",
    "clippy": "cargo clippy --manifest-path ./src-tauri/Cargo.toml --no-default-features -- -D warnings",
    "codefix": "yarn format && yarn clippy && yarn lint"
  },
  "dependencies": {},
  "devDependencies": {
    "@tauri-apps/api": "^1.0.2",
    "@tauri-apps/cli": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.23.0",
    "typescript": "^4.8.2",
    "uglify-js": "3",
    "vite": "^3.0.2"
  }
}
